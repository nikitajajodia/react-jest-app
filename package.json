{
  "name": "react-jest-app",
  "version": "1.0.0",
  "description": "A react app with JEST and Enzyme",
  "author": "Nikita-Jajodia",
  "repository": "https://github.com/nikitajajodia/react-redux-boilerplate.git",
  "scripts": {
    "dev": "webpack-dev-server --historyApiFallback true --port 8888 --content-base build/",
    "test": "jest",
    "updatetest": "npm test -- -u",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "setupFiles": [
      "./setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "html-webpack-plugin": "3.2.0",
    "immutable": "^3.8.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.16.0",
    "save": "^2.3.2",
    "webpack": "^4.16.5"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-1": "^6.24.1",
    "enzyme": "3.4.1",
    "enzyme-adapter-react-16": "1.2.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.4.2",
    "react-test-renderer": "16.4.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
